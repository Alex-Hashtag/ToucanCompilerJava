package org.alex_hashtag.shapes;

/**
 * This file has a public class Circle, and we can define more members if desired.
 */

// Circle is going to implement area
public class Circle
{
    private float64 radius = 1.0;

    public constructor new(float64 radius)
    {
        this.radius = radius;
    }

    public float64 calculateArea()
    {
        return 3.14159 * this.radius * this.radius;
    }
}
