package org.alex_hashtag.shapes;

/**
 * Another shape in a separate file.
 * We'll star-import from shapes, or do single imports
 */

public class Square
{
    private int32 sideLength = 1;

    public constructor new(int32 side)
    {
        this.sideLength = side;
    }

    public int32 perimeter()
    {
        return 4 * this.sideLength;
    }

    public int32 area()
    {
        return this.sideLength * this.sideLength;
    }
}
